create table citas (id bigint not null auto_increment, estado_cita bit, fecha_asignacion date, cliente_id_cliente varchar(255), primary key (id)) engine=InnoDB;
create table clientes (id_cliente varchar(255) not null, apellidos varchar(100), caracteristicas varchar(200), direccion varchar(100), fecha_creacion date not null, nombres varchar(100) not null, sexo varchar(1), telefono varchar(10), primary key (id_cliente)) engine=InnoDB
create table municipios (id bigint not null auto_increment, nombre varchar(255), primary key (id)) engine=InnoDB